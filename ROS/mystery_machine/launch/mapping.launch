<launch>


	<!-- SENSORS -->

	<!-- Lidar -->
	<node pkg="hokuyo_node" type="hokuyo_node" name="hokuyo_node" >
		<param name="port" value="/dev/ttyACM2" />
    </node>


	<!-- TRANSFORMS -->

	<!-- Lidar Transform -->
	<node pkg="mystery_machine" type="lidar_tf_publisher" name="lidar_tf_publisher" />

	<!-- Sonar Transform -->
	<node pkg="mystery_machine" type="sonar_tf_publisher" name="sonar_tf_publisher" />

	<!-- Transformed Sonar Publisher -->
	<node pkg="mystery_machine" type="sonar_transform" name="sonar_transform" />

	<!-- Odometry Publisher -->
	<node pkg="mystery_machine" type="odometry_publisher" name="odometry_publisher" />


	<!-- ARDUINO -->	

	<!-- Rosserial -->
	<node pkg="rosserial_python" type="serial_node.py" name="serial_node" args="/dev/ttyACM0" />


	<!-- MAPPING -->
	<node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" >
		<param name="scan" value="scan" />
	</node>

</launch>
